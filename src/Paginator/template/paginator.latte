{templateType Ages\Grid\Paginator\PaginatorTemplate}
{if $show === true}
    <div n:class="$gs->paginatorContainer()" n:snippet="paginator">
        <div n:class="$gs->paginatorPagesContainer()">
            <a href="{link paginate! ($paginator->getPage()-1)}" data-naja-history="off" n:class="$gs->paginatorIconLink()">
                <svg n:class="$gs->paginatorIconSize()" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
                    <path fill-rule="evenodd" d="M12.707 5.293a1 1 0 010 1.414L9.414 10l3.293 3.293a1 1 0 01-1.414 1.414l-4-4a1 1 0 010-1.414l4-4a1 1 0 011.414 0z" clip-rule="evenodd"/>
                </svg>
            </a>
            {foreach $steps as $step}
                {if $paginator->getPage() === $step}
                    <span n:class="$gs->paginatorActual()">{$step}</span>
                {else}
                    <a href="{link paginate! $step}" data-naja-history="off" n:class="$gs->paginatorLink()">{$step}</a>
                {/if}
                {if $iterator->getNextValue() > $step + 1}<span n:class="$gs->paginatorText()">â€¦</span>{/if}
            {/foreach}
            <a href="{link paginate! ($paginator->getPage()+1)}" data-naja-history="off" n:class="$gs->paginatorIconLink()">
                <svg n:class="$gs->paginatorIconSize()" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
                    <path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"/>
                </svg>
            </a>
        </div>

        <div n:class="$gs->paginatorSummaryContainer()">
        <span n:class="$gs->paginatorSummaryIconContainer()">
            <svg n:class="$gs->paginatorIconSize()" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" d="M8 7v8a2 2 0 002 2h6M8 7V5a2 2 0 012-2h4.586a1 1 0 01.707.293l4.414 4.414a1 1 0 01.293.707V15a2 2 0 01-2 2h-2M8 7H6a2 2 0 00-2 2v10a2 2 0 002 2h8a2 2 0 002-2v-2"/>
            </svg>
        </span>
            {foreach $itemsOptions as $item}
                {if $paginator->getItemsPerPage() === $item}
                    <span n:class="$gs->paginatorActual()">{$item}</span>
                {else}
                    <a href="{link items! $item}" data-naja-history="off" n:class="$gs->paginatorLink()">{$item}</a>
                {/if}
            {/foreach}
            <span n:class="$gs->paginatorSummaryContainer()">
                <span n:class="$gs->paginatorSummary()">{$gs->paginatorSummaryText()}</span>
                {$paginator->getItemCount()}
            </span>
        </div>
    </div>
{/if}
